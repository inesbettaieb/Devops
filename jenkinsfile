pipeline{
    
    agent any 
    
    stages {
        
        stage('Git Checkout'){
            
            steps{
                
                script{
                    
                    git branch: 'main', url: 'https://github.com/inesbettaieb/Devops.git'
                }
            }
        }
         stage('UNIT testing'){
            
            steps{
                
                sh """mvn test"""
            }
        }
    }
}        